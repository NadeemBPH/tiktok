{
  "name": "tiktok-scraper",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "deploy": "node server.js",
    "postinstall": "[ -f node_modules/puppeteer/install.js ] && node node_modules/puppeteer/install.js || echo 'Skipping Puppeteer install'"
  },
  "keywords": [
    "tiktok",
    "scraper",
    "puppeteer",
    "vpn",
    "proxy",
    "social-media",
    "data-extraction"
  ],
  "author": "",
  "license": "ISC",
  "description": "Enhanced TikTok Scraper with VPN/proxy support for regions where TikTok is blocked",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "puppeteer": "^21.7.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6"
  },
  "puppeteer": {
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--no-first-run",
      "--no-zygote",
      "--single-process",
      "--disable-gpu",
      "--disable-web-security",
      "--disable-blink-features=AutomationControlled",
      "--window-size=1920,1080",
      "--start-maximized",
      "--disable-popup-blocking",
      "--disable-notifications",
      "--disable-extensions",
      "--mute-audio"
    ]
  }
}
